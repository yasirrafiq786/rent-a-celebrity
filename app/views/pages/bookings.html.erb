  <% @bookings.each do |booking|%>
    <ol>
      <li><%= booking.celebrity.name %> - Owner: <%= booking.celebrity.user.email%> Requesting: <%= booking.user.email%> - Status: <%= booking.status%> 
      <% if current_user == booking.celebrity.user %>
      <%= link_to 'Confirm', confirm_path(booking), method: :patch %></li>
      <% end %>
    </ol>
  <% end%>
