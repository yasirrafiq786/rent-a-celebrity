<div class="container">
  <div class="row">
    <div class="col-6 offset-sm-2">
      <%= simple_form_for [celebrity, booking] do |f| %>
        <%= f.input :start_date, as: :date %>
        <%= f.input :end_date, as: :date %>
        <%= f.button :submit, class: 'btn btn-primary', data: {confirm: "Are You Sure"} %>
      <% end%>
    </div>
  </div>
</div>
